<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frag_home"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.HomeFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layout_appBar_home"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/app_bg"
        android:paddingVertical="10dp"
        android:paddingHorizontal="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent">

        <ImageView
            android:id="@+id/iv_barcode"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/ic_appbar_barcode"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>

        <ImageView
            android:id="@+id/iv_rogo"
            android:layout_width="100dp"
            android:layout_height="29dp"
            android:src="@drawable/ic_appbar_rogo"
            android:foregroundGravity="center"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
        <ImageView
            android:id="@+id/iv_mfeed"
            android:layout_width="60dp"
            android:layout_height="29dp"
            android:src="@drawable/ic_appbar_mfeed"
            android:layout_marginEnd="10dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/iv_menu"/>

        <ImageView
            android:id="@+id/iv_menu"
            android:layout_width="22dp"
            android:layout_height="22dp"
            android:src="@drawable/ic_menu"
            app:tint="@color/white"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="vertical"
        android:background="@color/app_bg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/layout_appBar_home"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/rv_top_event"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                android:background="@color/app_bg"
                android:layout_marginStart="5dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingVertical="15dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/shape_view_rounded"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/rv_top_event">

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/rv_category_movie_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginVertical="10dp"
                    android:layout_marginHorizontal="20dp"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/rv_category_movie"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginVertical="10dp"
                    android:layout_marginStart="10dp"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    app:layout_constraintStart_toStartOf="@id/rv_category_movie_title"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/rv_category_movie_title"/>

                <TextView
                    android:id="@+id/tv_more_movie"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="더 많은 영화보기"
                    android:textSize="12dp"
                    android:textColor="@color/category_txt"
                    android:drawableEnd="@drawable/ic_right_arrow"
                    android:drawablePadding="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginHorizontal="20dp"
                    app:layout_constraintTop_toBottomOf="@id/rv_category_movie"
                    app:layout_constraintStart_toStartOf="parent"/>

                <TextView
                    android:id="@+id/tv_movie_feed"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="무비피드"
                    android:textColor="@color/black"
                    android:textSize="20dp"
                    android:textStyle="bold"
                    android:layout_marginTop="30dp"
                    android:layout_marginHorizontal="20dp"
                    android:drawableEnd="@drawable/ic_next"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv_more_movie"/>

                <ImageView
                    android:id="@+id/iv_movie_feed_big"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_movie_feed_big_img"
                    android:adjustViewBounds="true"
                    android:layout_marginTop="15dp"
                    android:paddingHorizontal="20dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv_movie_feed"
                    app:layout_constraintEnd_toEndOf="parent"/>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/layout_movie_feed_week"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/shape_movie_item_rounded"
                    android:layout_marginTop="25dp"
                    android:layout_marginHorizontal="20dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/iv_movie_feed_big"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:elevation="7dp">

                    <ImageView
                        android:id="@+id/iv_movie_feed_small"
                        android:layout_width="95dp"
                        android:layout_height="95dp"
                        android:background="@drawable/shape_left_rounded"
                        android:src="@drawable/ic_movie_feed_small_img"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <TextView
                        android:id="@+id/tv_movie_feed_week"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="이번주 박스오피스"
                        android:textSize="15dp"
                        android:textColor="@color/black"
                        android:layout_marginTop="20dp"
                        android:layout_marginStart="20dp"
                        app:layout_constraintStart_toEndOf="@+id/iv_movie_feed_small"
                        app:layout_constraintTop_toTopOf="parent"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="국내는 물론 해외까지! 지금 관람객에게 사랑\n받는 영화는 무엇일까? 1위는 육사오 그럼..."
                        android:textColor="@color/category_txt"
                        android:textSize="12dp"
                        android:maxLines="2"
                        android:ellipsize="end"
                        android:layout_marginHorizontal="20dp"
                        android:layout_marginTop="5dp"
                        app:layout_constraintStart_toEndOf="@id/iv_movie_feed_small"
                        app:layout_constraintTop_toBottomOf="@id/tv_movie_feed_week"
                        app:layout_constraintEnd_toEndOf="parent"/>

                </androidx.constraintlayout.widget.ConstraintLayout>

                <LinearLayout
                    android:id="@+id/layout_movie_feed"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="30dp"
                    android:layout_marginHorizontal="20dp"
                    android:gravity="center"
                    android:weightSum="1"
                    android:background="@color/white"
                    android:elevation="7dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/layout_movie_feed_week">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="포토카드"
                        android:textColor="@color/category_txt"
                        android:textSize="12dp"
                        android:gravity="center"
                        android:layout_weight="0.20"
                        android:paddingVertical="13dp"
                        android:drawableTop="@drawable/ic_movie_feed_1"
                        android:drawablePadding="5dp"/>

                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="@color/line"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="극장"
                        android:textColor="@color/category_txt"
                        android:textSize="12dp"
                        android:gravity="center"
                        android:layout_weight="0.33"
                        android:paddingVertical="13dp"
                        android:drawableTop="@drawable/ic_movie_feed_2"
                        android:drawablePadding="5dp"/>

                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="@color/line"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="이벤트"
                        android:textColor="@color/category_txt"
                        android:textSize="12dp"
                        android:gravity="center"
                        android:layout_weight="0.32"
                        android:paddingVertical="13dp"
                        android:drawableTop="@drawable/ic_movie_feed_3"
                        android:drawablePadding="5dp"/>

                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="@color/line"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="본영화등록"
                        android:textColor="@color/category_txt"
                        android:textSize="12dp"
                        android:gravity="center"
                        android:layout_weight="0.15"
                        android:paddingVertical="13dp"
                        android:drawableTop="@drawable/ic_movie_feed_4"
                        android:drawablePadding="5dp"/>

                </LinearLayout>

                <ImageView
                    android:id="@+id/ad1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ad1"
                    android:adjustViewBounds="true"
                    android:layout_marginTop="30dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/layout_movie_feed"
                    app:layout_constraintEnd_toEndOf="parent"/>


                <TextView
                    android:id="@+id/tv_stage_greetings"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="무대인사 &amp; 시사"
                    android:textColor="@color/black"
                    android:textSize="20dp"
                    android:textStyle="bold"
                    android:layout_marginTop="40dp"
                    android:layout_marginHorizontal="20dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/ad1"/>

                <HorizontalScrollView
                    android:id="@+id/scrollView_stage_greetings"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginVertical="15dp"
                    android:scrollbarSize="0dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv_stage_greetings">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="330dp"
                            android:layout_height="160dp"
                            android:layout_marginStart="20dp"
                            android:layout_marginEnd="10dp"
                            android:layout_marginBottom="15dp"
                            android:background="@drawable/shape_movie_item_rounded"
                            android:elevation="5dp"
                            android:src="@drawable/ic_stage_greetings_1"/>

                        <ImageView
                            android:layout_width="330dp"
                            android:layout_height="160dp"
                            android:layout_marginEnd="10dp"
                            android:layout_marginBottom="15dp"
                            android:background="@drawable/shape_movie_item_rounded"
                            android:elevation="5dp"
                            android:src="@drawable/ic_stage_greetings_2"/>

                        <ImageView
                            android:layout_width="330dp"
                            android:layout_height="160dp"
                            android:layout_marginEnd="10dp"
                            android:layout_marginBottom="15dp"
                            android:background="@drawable/shape_movie_item_rounded"
                            android:elevation="5dp"
                            android:src="@drawable/ic_stage_greetings_3"/>

                        <ImageView
                            android:layout_width="330dp"
                            android:layout_height="160dp"
                            android:layout_marginEnd="10dp"
                            android:layout_marginBottom="15dp"
                            android:background="@drawable/shape_movie_item_rounded"
                            android:elevation="5dp"
                            android:src="@drawable/ic_stage_greetings_4"/>

                        <ImageView
                            android:layout_width="330dp"
                            android:layout_height="160dp"
                            android:layout_marginEnd="10dp"
                            android:layout_marginBottom="15dp"
                            android:background="@drawable/shape_movie_item_rounded"
                            android:elevation="5dp"
                            android:src="@drawable/ic_stage_greetings_5"/>

                    </LinearLayout>
                </HorizontalScrollView>

                <View
                    android:id="@+id/line_stage_greetings"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/line"
                    android:layout_marginTop="30dp"
                    android:layout_marginHorizontal="20dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/scrollView_stage_greetings"
                    app:layout_constraintEnd_toEndOf="parent"/>


                <TextView
                    android:id="@+id/tv_event"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="이벤트"
                    android:textColor="@color/black"
                    android:textSize="20dp"
                    android:textStyle="bold"
                    android:layout_marginTop="40dp"
                    android:layout_marginHorizontal="20dp"
                    android:drawableEnd="@drawable/ic_next"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/line_stage_greetings"/>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/rv_category_event_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="15dp"
                    android:layout_marginHorizontal="20dp"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    app:layout_constraintTop_toBottomOf="@id/tv_event"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/rv_category_event"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginVertical="20dp"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/rv_category_event_title"/>

                <View
                    android:id="@+id/line_event"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/line"
                    android:layout_marginTop="30dp"
                    android:layout_marginHorizontal="20dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/rv_category_event"
                    app:layout_constraintEnd_toEndOf="parent"/>


                <TextView
                    android:id="@+id/tv_special_official"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="특별관"
                    android:textColor="@color/black"
                    android:textSize="20dp"
                    android:textStyle="bold"
                    android:layout_marginTop="40dp"
                    android:layout_marginHorizontal="20dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/line_event"/>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/rv_special_official"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginVertical="20dp"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv_special_official"/>

                <View
                    android:id="@+id/line_special_official"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/line"
                    android:layout_marginTop="30dp"
                    android:layout_marginHorizontal="20dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/rv_special_official"
                    app:layout_constraintEnd_toEndOf="parent"/>


                <TextView
                    android:id="@+id/tv_movie_post"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="무비 포스트"
                    android:textColor="@color/black"
                    android:textSize="20dp"
                    android:textStyle="bold"
                    android:layout_marginTop="30dp"
                    android:layout_marginHorizontal="20dp"
                    android:drawableEnd="@drawable/ic_next"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/line_special_official"/>

                <TextView
                    android:id="@+id/tv_movie_post_string"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="영화를 관람하셨나요?\n나만의 무비포스트를 남겨보세요."
                    android:textColor="@color/category_txt"
                    android:textSize="15dp"
                    android:textStyle="bold"
                    android:layout_marginTop="20dp"
                    android:layout_marginStart="20dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv_movie_post"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="포스트 쓰기"
                    android:textColor="@color/category_txt"
                    android:textSize="13dp"
                    android:layout_marginTop="30dp"
                    android:layout_marginEnd="20dp"
                    android:paddingVertical="5dp"
                    android:paddingHorizontal="8dp"
                    android:background="@drawable/shape_textview_edge"
                    app:layout_constraintTop_toBottomOf="@id/tv_movie_post"
                    app:layout_constraintEnd_toEndOf="parent"/>

                <HorizontalScrollView
                    android:id="@+id/scrollView_post"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginVertical="15dp"
                    android:scrollbarSize="0dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv_movie_post_string">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="160dp"
                            android:layout_height="300dp"
                            android:background="@drawable/shape_movie_item_rounded"
                            android:elevation="5dp"
                            android:layout_marginStart="20dp"
                            android:layout_marginEnd="10dp"
                            android:layout_marginBottom="10dp"
                            android:src="@drawable/ic_movie_post_1"/>
                        <ImageView
                            android:layout_width="160dp"
                            android:layout_height="300dp"
                            android:background="@drawable/shape_movie_item_rounded"
                            android:elevation="5dp"
                            android:layout_marginEnd="10dp"
                            android:layout_marginBottom="10dp"
                            android:src="@drawable/ic_movie_post_2"/>
                        <ImageView
                            android:layout_width="160dp"
                            android:layout_height="300dp"
                            android:background="@drawable/shape_movie_item_rounded"
                            android:elevation="5dp"
                            android:layout_marginEnd="10dp"
                            android:layout_marginBottom="10dp"
                            android:src="@drawable/ic_movie_post_3"/>
                    </LinearLayout>

                </HorizontalScrollView>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/bottom_bg"
                    android:layout_marginTop="40dp"
                    android:paddingBottom="20dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/scrollView_post"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent">

                    <TextView
                        android:id="@+id/bottom_str1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="이용약관"
                        android:textSize="11dp"
                        android:textColor="@color/category_txt"
                        android:layout_marginTop="30dp"
                        android:layout_marginStart="50dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <View
                        android:id="@+id/bottom_line1"
                        android:layout_width="1dp"
                        android:layout_height="10dp"
                        android:background="@color/line"
                        android:layout_marginStart="10dp"
                        android:paddingVertical="5dp"
                        app:layout_constraintTop_toTopOf="@id/bottom_str1"
                        app:layout_constraintStart_toEndOf="@id/bottom_str1"/>
                    <TextView
                        android:id="@+id/bottom_str2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="위치기반서비스 이용약관"
                        android:textSize="11dp"
                        android:textColor="@color/category_txt"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="30dp"
                        app:layout_constraintStart_toEndOf="@id/bottom_line1"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <View
                        android:id="@+id/bottom_line2"
                        android:layout_width="1dp"
                        android:layout_height="10dp"
                        android:background="@color/line"
                        android:layout_marginStart="5dp"
                        android:paddingVertical="3dp"
                        app:layout_constraintTop_toTopOf="@id/bottom_str2"
                        app:layout_constraintStart_toEndOf="@id/bottom_str2"/>
                    <TextView
                        android:id="@+id/bottom_str3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="개인정보처리방침"
                        android:textSize="11dp"
                        android:textColor="@color/app_btn"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="30dp"
                        app:layout_constraintStart_toEndOf="@id/bottom_line2"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <TextView
                        android:id="@+id/bottom_str4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="고객센터"
                        android:textSize="11dp"
                        android:textColor="@color/category_txt"
                        android:layout_marginTop="20dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/bottom_str2"
                        app:layout_constraintEnd_toEndOf="parent"/>


                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>